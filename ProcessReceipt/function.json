{
  "bindings": [
    {
      "name": "msgReceipt",
      "type": "queueTrigger",
      "direction": "in",
      "queueName": "%NOTIFICATION_RECEIPT_QUEUE%",
      "connection": "AzureWebJobsStorage"
    },
    {
      "name": "receipt",
      "type": "cosmosDB",
      "direction": "out",
      "databaseName": "gwa",
      "collectionName": "%COSMOS_DB_RECEIPTS_CONTAINER%",
      "connectionStringSetting": "COSMOS_DB"
    }
  ],
  "disabled": false
}
