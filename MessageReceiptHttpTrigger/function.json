{
  "bindings": [
    {
      "authLevel": "function",
      "type": "httpTrigger",
      "direction": "in",
      "name": "req",
      "methods": [
        "post"
      ]
    },
    {
      "type": "http",
      "direction": "out",
      "name": "res"
    },
    {
      "name": "messagesToSend",
      "type": "queue",
      "direction": "out",
      "queueName": "%MSG_RECEIPT_QUEUE%",
      "connection": "AzureWebJobsStorage"
    }
  ]
}
